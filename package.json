{
  "name": "@epicurrents/core",
  "version": "0.0.2-alpha.46",
  "description": "EpiCurrents core library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./assets": "./dist/assets/index.js",
    "./config": "./dist/config/index.js",
    "./errors": "./dist/errors/index.js",
    "./onnx": "./dist/onnx/index.js",
    "./plots": "./dist/plots/index.js",
    "./pyodide": "./dist/pyodide/index.js",
    "./runtime": "./dist/runtime/index.js",
    "./types": "./dist/types/index.d.ts",
    "./util": "./dist/util/index.js",
    "./package.json": "./package.json",
    "./dist/assets": "./dist/assets/index.js",
    "./dist/config": "./dist/config/index.js",
    "./dist/errors": "./dist/errors/index.js",
    "./dist/onnx": "./dist/onnx/index.js",
    "./dist/plots": "./dist/plots/index.js",
    "./dist/pyodide": "./dist/pyodide/index.js",
    "./dist/runtime": "./dist/runtime/index.js",
    "./dist/types": "./dist/types/index.d.ts",
    "./dist/util": "./dist/util/index.js"
  },
  "imports": {
    "#runtime*": "./dist/src/runtime/index.ts",
    "#root/*": "./dist/*",
    "#*": "./dist/src/*"
  },
  "files": [
    "dist/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "node node_modules/typescript/bin/tsc && node node_modules/tsconfig-replace-paths/dist/commonjs --project tsconfig.json",
    "build:webpack": "node node_modules/webpack/bin/webpack.js --progress --config webpack.build.js",
    "dev": "node node_modules/webpack/bin/webpack.js --progress --watch --config webpack.dev.js",
    "lint": "npm run lint:src",
    "lint:src": "node node_modules/eslint/bin/eslint src",
    "prepack": "npm run build",
    "serve": "node node_modules/webpack/bin/webpack.js serve --progress --config webpack.dev.js",
    "test": "npm run test:unit",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --verbose --no-cache --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epicurrents/core.git"
  },
  "author": "Sampsa Lohi",
  "license": "Apache-2.0",
  "dependencies": {
    "@stdlib/constants-float32": "^0.0.6",
    "asymmetric-io-mutex": "^0.7.0",
    "codecutils": "^0.1.3",
    "d3-interpolate": "^3.0.1",
    "downsample": "^1.4.0",
    "fili": "^2.0.3",
    "onnxruntime-web": "^1.14.0",
    "pyodide": "0.19.0",
    "scoped-ts-log": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@microsoft/tsdoc": "^0.14.1",
    "@types/jest": "^27.5.2",
    "@types/offscreencanvas": "^2019.7.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.5",
    "circular-dependency-plugin": "^5.2.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.49.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.3.0",
    "tsconfig-replace-paths": "^0.0.14",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0"
  }
}
