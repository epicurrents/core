{
  "name": "@epicurrents/core",
  "version": "0.3.0-0",
  "description": "Epicurrents core library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./assets": "./dist/assets/index.js",
    "./config": "./dist/config/index.js",
    "./events": "./dist/events/index.js",
    "./plots": "./dist/plots/index.js",
    "./runtime": "./dist/runtime/index.js",
    "./types": "./dist/types/index.d.ts",
    "./util": "./dist/util/index.js",
    "./workers": "./dist/workers/index.js",
    "./package.json": "./package.json",
    "./dist/assets": "./dist/assets/index.js",
    "./dist/config": "./dist/config/index.js",
    "./dist/events": "./dist/events/index.js",
    "./dist/plots": "./dist/plots/index.js",
    "./dist/runtime": "./dist/runtime/index.js",
    "./dist/types": "./dist/types/index.d.ts",
    "./dist/util": "./dist/util/index.js",
    "./dist/workers": "./dist/workers/index.js"
  },
  "imports": {
    "#runtime*": "./dist/src/runtime/index.ts",
    "#*": "./dist/*"
  },
  "files": [
    "dist/*",
    "umd/*.js",
    "umd/*.LICENSE.txt",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:umd && npm run build:tsc",
    "build:docs": "npx typedoc",
    "build:tsc": "node node_modules/typescript/bin/tsc && node node_modules/tsconfig-replace-paths/dist/commonjs --project tsconfig.json",
    "build:umd": "node node_modules/webpack/bin/webpack.js --progress --stats-error-details --config webpack.config.js",
    "dev": "node node_modules/webpack/bin/webpack.js serve --progress --config webpack.dev.js",
    "lint": "npm run lint:src",
    "lint:src": "node node_modules/eslint/bin/eslint src",
    "prepack": "npm run build",
    "release": "npm publish --access=public",
    "serve": "node node_modules/webpack/bin/webpack.js serve --progress --config webpack.dev.js",
    "test": "npm run test:unit",
    "test:unit": "jest --coverage --verbose --no-cache --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epicurrents/core.git"
  },
  "author": "Sampsa Lohi",
  "license": "Apache-2.0",
  "dependencies": {
    "@stdlib/constants-float16": "^0.2.1",
    "@stdlib/constants-float32": "^0.2.1",
    "@stdlib/constants-float64": "^0.2.1",
    "asymmetric-io-mutex": "^0.9.0",
    "codecutils": "^0.1.3",
    "downsample": "^1.4.0",
    "fili": "^2.0.3",
    "onnxruntime-web": "^1.14.0",
    "scoped-event-bus": "^0.2.1",
    "scoped-event-log": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@microsoft/tsdoc": "^0.14.1",
    "@types/jest": "^29.5.14",
    "@types/offscreencanvas": "^2019.7.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "babel-loader": "^8.2.5",
    "circular-dependency-plugin": "^5.2.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.3.0",
    "tsconfig-replace-paths": "^0.0.14",
    "typedoc": "^0.27.2",
    "typescript": "5.6",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0"
  }
}
