{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "lib": [
      "es5", "es6", "esnext",
      "webworker",
    ],
    "strict": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "#@/*": ["src/*"],
      "#assets/*": ["src/assets/*"],
      "#config/*": ["src/config/*"],
      "#root/*": ["./*"],
      "#runtime*": ["src/runtime/index.ts"],
      "#types/*": ["src/types/*"],
      "#util/*": ["src/util/*"],
    } ,
    "typeRoots": [
      "./node_modules/@types",
    ],
    "types": [
      "node",
      "jest",
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
}
